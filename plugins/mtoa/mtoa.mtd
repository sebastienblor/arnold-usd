# options

[node options]
    # actully a UINT
	maya.id         INT    0x00115C00
	# for all attributes unless specified
	# maya.keyable    BOOL   False
    [attr AA_samples]
        maya.keyable    BOOL   False
    	min         INT     0
    	max			INT     100
    	softmin		INT     1
        softmax		INT     10
        # default		INT     3   
		maya.orig_shortname	STRING	"AAsmpls"
	[attr bucket_scanning]
        maya.orig_shortname	STRING	"bktsc"
	[attr bucket_size]
		maya.orig_shortname	STRING	"bktsz"
    	min			INT		1
    	softmin		INT		1
        softmax		INT		256		
	[attr abort_on_error]
		maya.orig_shortname	STRING	"abort"
	[attr abort_on_license_fail]
		maya.orig_shortname	STRING	"abortlic"
	[attr AA_sample_clamp]
    	# softmin		FLOAT	0.001
        # softmax		FLOAT	100.0
        # default		FLOAT	10.0	
		maya.orig_shortname	STRING	"smpclamp"
	[attr ignore_textures]
		# maya.keyable	BOOL	False
	[attr ignore_shaders]
		# maya.keyable	BOOL	False
	[attr ignore_lights]
