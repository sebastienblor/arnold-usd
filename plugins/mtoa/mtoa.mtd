# options

[node options]
    maya.name               STRING  "aiOptions"
    maya.id                 INT     0x00115C00
    [attr AA_samples]
        maya.keyable        BOOL    false
        min                 INT     -10
        max                 INT     100
        softmin             INT     1
        softmax             INT     10
        default             INT     3
        linkable            BOOL    false
        maya.orig_shortname STRING  "AAsmpls"
    [attr bucket_size]
        maya.keyable        BOOL    false
        min                 INT     16
        softmin             INT     16
        softmax             INT     256
        linkable            BOOL    false
        maya.orig_shortname STRING  "bktsz"
    [attr abort_on_error]
        maya.keyable        BOOL    false
        linkable            BOOL    false
        maya.orig_shortname STRING  "abort"
    [attr abort_on_license_fail]
        maya.keyable        BOOL    false
        linkable            BOOL    false
        maya.orig_shortname STRING  "abortlic"
    [attr AA_sample_clamp]
        maya.keyable        BOOL    false
        softmin             FLOAT   0.001
        softmax             FLOAT   100.0
        default             FLOAT   10.0
        linkable            BOOL    false
        maya.orig_shortname STRING  "smpclamp"
