	[attr ignore_motion_blur]
		# maya.keyable	BOOL	False
	[attr ignore_smoothing]
		# maya.keyable	BOOL	False
	[attr ignore_sss]
		# maya.keyable	BOOL	False
	[attr auto_transparency_depth]
		min			INT		0
    	softmin		INT		0
        softmax		INT		16
		maya.orig_shortname	STRING	"at_d"
	[attr auto_transparency_threshold]
		min			FLOAT	0.0
		max			FLOAT	10.0
		maya.orig_shortname	STRING	"at_t"
	[attr auto_transparency_probabilistic]
		maya.orig_shortname	STRING	"at_p"
	[attr sss_subpixel_cache]
