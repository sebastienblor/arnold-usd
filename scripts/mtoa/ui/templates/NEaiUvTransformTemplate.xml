		<attribute name='pivotFrame' type='maya.float2'>
			<label>Pivot Frame</label>
		</attribute>
		<attribute name='wrapFrameColor' type='maya.float3'>
			<label>Wrap Frame Color</label>
		</attribute>
		<attribute name='repeat' type='maya.float2'>
