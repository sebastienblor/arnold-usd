<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiUvTransform'>
		<attribute name='passthrough' type='maya.float3'>
			<label>Passthrough</label>
		</attribute>
		<attribute name='coverage' type='maya.float2'>
			<label>Coverage</label>
		</attribute>
		<attribute name='scaleFrame' type='maya.float2'>
			<label>Scale Frame</label>
