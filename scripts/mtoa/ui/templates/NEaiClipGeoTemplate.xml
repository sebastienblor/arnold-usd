<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEaiClipGeo'>
        <attribute name='outColor' type='maya.float3'>
            <label>Out Color</label>
        </attribute>
        <attribute name='intersection' type='maya.float3'>
            <label>Intersection</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEaiClipGeo'>
        <property name='outColor'/>
        <property name='intersection'/>
    </view>
    <view name='NEDefaultSoloOutput' template='NEaiClipGeo'>
        <property name='outColor'/>
    </view>

</templates>