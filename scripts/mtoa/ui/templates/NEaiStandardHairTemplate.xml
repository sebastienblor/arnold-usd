		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='base' type='maya.float'>
			<label>Base</label>
		</attribute>
		<attribute name='baseColor' type='maya.float3'>
			<label>Base Color</label>
		</attribute>
		<attribute name='melanin' type='maya.float'>
			<label>Melanin</label>
		</attribute>
		<attribute name='melaninRedness' type='maya.float'>
			<label>Melanin Redness</label>
		</attribute>
		<attribute name='melaninRandomize' type='maya.float'>
			<label>Melanin Randomize</label>
		</attribute>
		<attribute name='roughness' type='maya.float'>
