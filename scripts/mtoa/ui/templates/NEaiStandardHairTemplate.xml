	<view name='NEDefault' template='NEaiStandardHair'>
		<property name='outColor'/>
		<property name='outAlpha'/>
		<property name='base'/>
		<property name='baseColor'/>
		<property name='melanin'/>
		<property name='melaninRedness'/>
		<property name='melaninRandomize'/>
		<property name='roughness'/>
