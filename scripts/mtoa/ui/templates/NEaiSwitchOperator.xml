<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEaiSwitchOperator'>
        <attribute name='outColor' type='maya.message'>
            <label>Out Color</label>
        </attribute>
        <attribute name='inputs' type='maya.message'>
            <label>Inputs</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEaiSwitchOperator'>
        <property name='outColor'/>
        <property name='inputs'/>
    </view>
    <view name='NEDefaultSoloOutput' template='NEaiSwitchOperator'>
        <property name='outColor'/>
    </view>
</templates>