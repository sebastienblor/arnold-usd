<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiGobo'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='slidemap' type='maya.float3'>
			<label>Slidemap</label>
		</attribute>
		<attribute name='density' type='maya.float'>
			<label>Density</label>
