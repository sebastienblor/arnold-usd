<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiAdd'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='input1' type='maya.float3'>
			<label>Input 1</label>
		</attribute>
		<attribute name='input2' type='maya.float3'>
			<label>Input 2</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiAdd'>
		<property name='outColor'/>
		<property name='input1'/>
		<property name='input2'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaiAdd'>
		<property name='outColor'/>
	</view>
</templates>