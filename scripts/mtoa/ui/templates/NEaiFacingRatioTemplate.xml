<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiFacingRatio'>
		<attribute name='outValue' type='maya.float'>
			<label>Out Value</label>
		</attribute>
		<attribute name='bias' type='maya.float'>
			<label>Bias</label>
		</attribute>
		<attribute name='gain' type='maya.float'>
			<label>Gain</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiFacingRatio'>
		<property name='outValue'/>
		<property name='bias'/>
		<property name='gain'/>
	</view>
</templates>