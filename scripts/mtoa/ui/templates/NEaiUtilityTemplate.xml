
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiUtility'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='color' type='maya.float3'>
			<label>Color</label>
		</attribute>
		<attribute name='normal' type='maya.float3'>
			<label>Normal</label>
		</attribute>	
	</template>
	<view name='NEDefault' template='NEaiUtility'>
		<property name='outColor'/>
		<property name='color'/>
		<property name='normal'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaiUtility'>
		<property name='outColor'/>
	</view>
</templates>
