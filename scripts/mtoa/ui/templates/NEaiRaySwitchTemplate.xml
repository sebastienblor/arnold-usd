<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiRaySwitch'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='camera' type='maya.float3'>
			<label>Camera</label>
		</attribute>
		<attribute name='shadow' type='maya.float3'>
			<label>Shadow</label>
		</attribute>
		<attribute name='diffuseReflection' type='maya.float3'>
			<label>Diffuse Reflection</label>
		</attribute>
		<attribute name='specularReflection' type='maya.float3'>
			<label>Specular Reflection</label>
		</attribute>
		<attribute name='diffuseTransmission' type='maya.float3'>
			<label>Diffuse Transmission</label>
		</attribute>
		<attribute name='specularTransmission' type='maya.float3'>
			<label>Specular Transmission</label>
		</attribute>
		<attribute name='volume' type='maya.float3'>
			<label>Volume</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiRaySwitch'>
		<property name='outColor'/>
		<property name='outAlpha'/>
		<property name='camera'/>
		<property name='shadow'/>
		<property name='diffuseReflection'/>
		<property name='specularReflection'/>
		<property name='diffuseTransmission'/>
		<property name='specularTransmission'/>
		<property name='volume'/>
	</view>
</templates>