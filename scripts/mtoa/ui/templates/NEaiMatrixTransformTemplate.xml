<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiMatrixTransform'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='outTransparency' type='maya.float3'>
			<label>Out Transparency</label>
		</attribute>
		<attribute name='outValue' type='maya.matrix'>
			<label>Out Value</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiMatrixTransform'>
		<property name='message'/>
		<property name='outTransparency'/>
		<property name='outValue'/>
	</view>
</templates>