<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiIsFinite'>
		<attribute name='outValue' type='maya.bool'>
			<label>Out Value</label>
		</attribute>
		<attribute name='input' type='maya.float3'>
			<label>Input</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiIsFinite'>
		<property name='outValue'/>
		<property name='input'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaiIsFinite'>
		<property name='outValue'/>
	</view>
</templates>