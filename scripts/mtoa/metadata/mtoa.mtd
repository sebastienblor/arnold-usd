# atmosphere

[node sky]
    desc		STRING	"Sky shader."
    [attr intensity]
    	desc		STRING	"Intensity of sky illumination."
    	min			FLOAT	0.0
        softmax		FLOAT	10.0
        default		FLOAT 	1.0

[node volume_scattering]
	desc STRING "Volume scattering shader."
    [attr affect_camera]
    	desc STRING "Scattering effect on camera rays."
    	min			FLOAT	0.0
    	max			FLOAT	1.0
    [attr affect_diffuse]
    	desc STRING "Scattering effect on diffuse rays."
    	min			FLOAT	0.0
    	max			FLOAT	1.0
    [attr affect_reflection]
    	desc STRING "Scattering effect on reflection rays."
    	min			FLOAT	0.0
    	max			FLOAT	1.0
    [attr attenuation]
    	desc STRING "Scattering attenuation."
    	min			FLOAT	0.0
    	max			FLOAT	1.0    	
    [attr density]
    	desc STRING "Scattering density."
    	min			FLOAT	0.0
    	max			FLOAT	1.0 
    [attr eccentricity]
    	desc STRING "Scattering eccentricity."
    	min			FLOAT	-1.0
    	max			FLOAT	1.0       	
    [attr mscattering_depth]
    	desc STRING "Scattering depth."
    	min			FLOAT	0.0
    	softmax		FLOAT	10.0    
    [attr mscattering_sample]
    	desc STRING "Scattering number of samples."
    	min			INT		0
    	softmax		INT		64

[node fog]
    [attr distance]
    	desc STRING "Fog full opacity distance."
    	min			FLOAT	0.0
    	softmax		FLOAT	1000.0  
	[attr height]
    	desc STRING "Fog full height."
    	min			FLOAT	0.0
    	softmax		FLOAT	1000.0  

