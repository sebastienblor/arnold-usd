# lights

[node point_light]
	[attr bounce_factor]
    	min			FLOAT	0.0
        softmax		FLOAT	20.0	
	[attr bounces]
        min			INT		0
        max			INT		10000
	[attr sss_samples]
        min			INT		0
        max			INT		100

[node spot_light]
	[attr bounce_factor]
    	min			FLOAT	0.0
        softmax		FLOAT	20.0
	[attr bounces]
        min			INT		0
        max			INT		10000
	[attr sss_samples]
        min			INT		0
        max			INT		100
	[attr aspect_ratio]
		min			FLOAT	0
        max			FLOAT	100	
		softmin		FLOAT	0
		softmax		FLOAT	1
	[attr lens_radius]
		min			FLOAT	0
        max			FLOAT	100	
		softmin		FLOAT	0
		softmax		FLOAT	1

